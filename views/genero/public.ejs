<%-include('../cabecalho.ejs')%>
<h1>Gêneros</h1>
<div class="genre-list">
  <% if (Items.length === 0) { %>
    <p>Nenhum gênero encontrado.</p>
  <% } else { %>
    <% for (const it of Items) { %>
      <div class="genre-item">
        <h3><%= it.nome %></h3>
        <p><%= it.descricao %></p>
      </div>
    <% } %>
  <% } %>
</div>
<%-include('../rodape.ejs')%>
