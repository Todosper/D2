<%-include('../cabecalho.ejs')%>
<h1>Músicas</h1>
<div class="music-list">
  <% if (Musicas.length === 0) { %>
    <p>Nenhuma música encontrada.</p>
  <% } else { %>
    <% for (const m of Musicas) { %>
      <div class="music-item">
        <h3><%= m.nome %></h3>
        <p><strong>Artista:</strong> <%= m.artista %></p>
        <p><strong>Gênero:</strong> <%= m.genero %></p>
      </div>
    <% } %>
  <% } %>
</div>
<%-include('../rodape.ejs')%>
